{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "actions",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "root_action"
      }
    },
    "_action_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "state",
          "content": {
            "type": "SYMBOL",
            "name": "state"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "name"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "metadata",
            "content": {
              "type": "SYMBOL",
              "name": "_metadata"
            }
          }
        }
      ]
    },
    "root_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth1_action"
            }
          }
        }
      ]
    },
    "depth1_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth2_action"
            }
          }
        }
      ]
    },
    "depth2_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>"
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth3_action"
            }
          }
        }
      ]
    },
    "depth3_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>"
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth4_action"
            }
          }
        }
      ]
    },
    "depth4_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>>"
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth5_action"
            }
          }
        }
      ]
    },
    "depth5_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>>>"
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        }
      ]
    },
    "state": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "state_not_started"
              },
              {
                "type": "SYMBOL",
                "name": "state_completed"
              },
              {
                "type": "SYMBOL",
                "name": "state_in_progress"
              },
              {
                "type": "SYMBOL",
                "name": "state_blocked"
              },
              {
                "type": "SYMBOL",
                "name": "state_cancelled"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "state_not_started": {
      "type": "STRING",
      "value": " "
    },
    "state_completed": {
      "type": "STRING",
      "value": "x"
    },
    "state_in_progress": {
      "type": "STRING",
      "value": "-"
    },
    "state_blocked": {
      "type": "STRING",
      "value": "="
    },
    "state_cancelled": {
      "type": "STRING",
      "value": "_"
    },
    "name": {
      "type": "PATTERN",
      "value": "[^\\n$!*+@%#>]+"
    },
    "_metadata": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "description"
        },
        {
          "type": "SYMBOL",
          "name": "priority"
        },
        {
          "type": "SYMBOL",
          "name": "story"
        },
        {
          "type": "SYMBOL",
          "name": "context"
        },
        {
          "type": "SYMBOL",
          "name": "do_date"
        },
        {
          "type": "SYMBOL",
          "name": "completed_date"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        }
      ]
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n!*+@%#>]+"
          }
        }
      ]
    },
    "priority": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "FIELD",
          "name": "level",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]+"
          }
        }
      ]
    },
    "story": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n!$+@%#>]+"
          }
        }
      ]
    },
    "context": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "SYMBOL",
            "name": "tag"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "FIELD",
                "name": "tag",
                "content": {
                  "type": "SYMBOL",
                  "name": "tag"
                }
              }
            ]
          }
        }
      ]
    },
    "tag": {
      "type": "PATTERN",
      "value": "[^,\\n!$*@%#>]+"
    },
    "do_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "FIELD",
          "name": "datetime",
          "content": {
            "type": "SYMBOL",
            "name": "datetime"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "duration",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "recurrence",
              "content": {
                "type": "SYMBOL",
                "name": "recurrence"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "datetime": {
      "type": "PATTERN",
      "value": "[0-9]{4}-[0-9]{2}-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?(Z|[+-][0-9]{2}:?[0-9]{2})?)?"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "D"
        },
        {
          "type": "FIELD",
          "name": "minutes",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]+"
          }
        }
      ]
    },
    "recurrence": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "R:"
        },
        {
          "type": "FIELD",
          "name": "rrule",
          "content": {
            "type": "PATTERN",
            "value": "FREQ=(SECONDLY|MINUTELY|HOURLY|DAILY|WEEKLY|MONTHLY|YEARLY)(;[A-Z]+(=[A-Z0-9,+-]+)?)*"
          }
        }
      ]
    },
    "completed_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "FIELD",
          "name": "datetime",
          "content": {
            "type": "SYMBOL",
            "name": "datetime"
          }
        }
      ]
    },
    "id": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "FIELD",
          "name": "uuid",
          "content": {
            "type": "PATTERN",
            "value": "[0-9a-fA-F-]+"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}