{
  "name": "actions",
  "rules": {
    "action_list": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "action"
      }
    },
    "action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "state"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "priority"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "state": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "not_started"
            },
            {
              "type": "SYMBOL",
              "name": "completed"
            },
            {
              "type": "SYMBOL",
              "name": "in_progress"
            },
            {
              "type": "SYMBOL",
              "name": "blocked"
            },
            {
              "type": "SYMBOL",
              "name": "cancelled"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "not_started": {
      "type": "STRING",
      "value": " "
    },
    "completed": {
      "type": "STRING",
      "value": "x"
    },
    "in_progress": {
      "type": "STRING",
      "value": "-"
    },
    "blocked": {
      "type": "STRING",
      "value": "="
    },
    "cancelled": {
      "type": "STRING",
      "value": "_"
    },
    "name": {
      "type": "PATTERN",
      "value": "[^$!*+@%>]+"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "PATTERN",
          "value": "[^!*+@%>]+"
        }
      ]
    },
    "priority": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "PATTERN",
          "value": "[0-9]+"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
