          "content": {
            "type": "STRING",
            "value": "U"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "iso_date_time"
              },
              {
                "type": "SYMBOL",
                "name": "iso_date"
              },
              {
