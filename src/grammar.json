      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "hour"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "minute"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "am"
                },
                {
                  "type": "STRING",
                  "value": "pm"
                },
                {
                  "type": "STRING",
                  "value": "AM"
                },
                {
                  "type": "STRING",
                  "value": "PM"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "hour": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "minute": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "D"
        },
        {
          "type": "SYMBOL",
          "name": "duration_value"
        }
      ]
    },
    "duration_value": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "id": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "uuid"
        }
      ]
    },
    "uuid": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "uuid_time_high"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
