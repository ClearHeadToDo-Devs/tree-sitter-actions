{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "actions",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "root_action"
      }
    },
    "_action_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "state",
          "content": {
            "type": "SYMBOL",
            "name": "state"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "name"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "metadata",
            "content": {
              "type": "SYMBOL",
              "name": "_metadata"
            }
          }
        }
      ]
    },
    "root_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth1_action"
            }
          }
        }
      ]
    },
    "depth1_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": ">"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth2_action"
            }
          }
        }
      ]
    },
    "depth2_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": ">>"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth3_action"
            }
          }
        }
      ]
    },
    "depth3_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": ">>>"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth4_action"
            }
          }
        }
      ]
    },
    "depth4_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": ">>>>"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "depth5_action"
            }
          }
        }
      ]
    },
    "depth5_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": ">>>>>"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_action_body"
        }
      ]
    },
    "state": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "open",
          "content": {
            "type": "STRING",
            "value": "["
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "state_not_started"
              },
              {
                "type": "SYMBOL",
                "name": "state_completed"
              },
              {
                "type": "SYMBOL",
                "name": "state_in_progress"
              },
              {
                "type": "SYMBOL",
                "name": "state_blocked"
              },
              {
                "type": "SYMBOL",
                "name": "state_cancelled"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "close",
          "content": {
            "type": "STRING",
            "value": "]"
          }
        }
      ]
    },
    "state_not_started": {
      "type": "STRING",
      "value": " "
    },
    "state_completed": {
      "type": "STRING",
      "value": "x"
    },
    "state_in_progress": {
      "type": "STRING",
      "value": "-"
    },
    "state_blocked": {
      "type": "STRING",
      "value": "="
    },
    "state_cancelled": {
      "type": "STRING",
      "value": "_"
    },
    "name": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "link"
          },
          {
            "type": "SYMBOL",
            "name": "name_text_chunk"
          }
        ]
      }
    },
    "name_text_chunk": {
      "type": "PATTERN",
      "value": "[^\\n$!*+@%#>\\[]+"
    },
    "_metadata": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "description"
        },
        {
          "type": "SYMBOL",
          "name": "priority"
        },
        {
          "type": "SYMBOL",
          "name": "story"
        },
        {
          "type": "SYMBOL",
          "name": "context"
        },
        {
          "type": "SYMBOL",
          "name": "do_date"
        },
        {
          "type": "SYMBOL",
          "name": "completed_date"
        },
        {
          "type": "SYMBOL",
          "name": "id"
        }
      ]
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "$"
          }
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "link"
                },
                {
                  "type": "SYMBOL",
                  "name": "description_text_chunk"
                }
              ]
            }
          }
        }
      ]
    },
    "description_text_chunk": {
      "type": "PATTERN",
      "value": "[^\\n!*+@%#>\\[]+"
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "text",
                  "content": {
                    "type": "SYMBOL",
                    "name": "link_text"
                  }
                },
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "FIELD",
                  "name": "url",
                  "content": {
                    "type": "SYMBOL",
                    "name": "link_url"
                  }
                }
              ]
            },
            {
              "type": "FIELD",
              "name": "url",
              "content": {
                "type": "SYMBOL",
                "name": "link_url"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]]"
        }
      ]
    },
    "link_text": {
      "type": "PATTERN",
      "value": "[^\\|\\]]+"
    },
    "link_url": {
      "type": "PATTERN",
      "value": "[^\\|\\]]+"
    },
    "priority": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "!"
          }
        },
        {
          "type": "FIELD",
          "name": "level",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]+"
          }
        }
      ]
    },
    "story": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "*"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n!$+@%#>]+"
          }
        }
      ]
    },
    "context": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "+"
          }
        },
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "SYMBOL",
            "name": "tag"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "FIELD",
                "name": "tag",
                "content": {
                  "type": "SYMBOL",
                  "name": "tag"
                }
              }
            ]
          }
        }
      ]
    },
    "tag": {
      "type": "PATTERN",
      "value": "[^,\\n!$*@%#>]+"
    },
    "do_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "@"
          }
        },
        {
          "type": "FIELD",
          "name": "datetime",
          "content": {
            "type": "SYMBOL",
            "name": "datetime"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "duration",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "recurrence",
              "content": {
                "type": "SYMBOL",
                "name": "recurrence"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "datetime": {
      "type": "PATTERN",
      "value": "[0-9]{4}-[0-9]{2}-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?(Z|[+-][0-9]{2}:?[0-9]{2})?)?"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "D"
        },
        {
          "type": "FIELD",
          "name": "minutes",
          "content": {
            "type": "SYMBOL",
            "name": "minutes"
          }
        }
      ]
    },
    "minutes": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "recurrence": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "R:"
        },
        {
          "type": "FIELD",
          "name": "rrule",
          "content": {
            "type": "SYMBOL",
            "name": "rrule_content"
          }
        }
      ]
    },
    "rrule_content": {
      "type": "PATTERN",
      "value": "FREQ=(SECONDLY|MINUTELY|HOURLY|DAILY|WEEKLY|MONTHLY|YEARLY)(;[A-Z]+(=[A-Z0-9,+-]+)?)*"
    },
    "completed_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "%"
          }
        },
        {
          "type": "FIELD",
          "name": "datetime",
          "content": {
            "type": "SYMBOL",
            "name": "datetime"
          }
        }
      ]
    },
    "uuid_value": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F-]+"
    },
    "id": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "FIELD",
          "name": "uuid",
          "content": {
            "type": "SYMBOL",
            "name": "uuid_value"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}