{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "actions",
  "rules": {
    "action_list": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "root_action"
      }
    },
    "root_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "story",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "FIELD",
                    "name": "icon",
                    "content": {
                      "type": "STRING",
                      "value": "*"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "text",
                    "content": {
                      "type": "SYMBOL",
                      "name": "safe_text"
                    }
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "child_action"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "child_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "STRING",
            "value": ">"
          }
        },
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "grandchild_action"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "STRING",
            "value": ">>"
          }
        },
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "great_grandchild_action"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "great_grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "STRING",
            "value": ">>>"
          }
        },
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "double_great_grandchild_action"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "double_great_grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "STRING",
            "value": ">>>>"
          }
        },
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "leaf_action"
                }
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "leaf_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "STRING",
            "value": ">>>>>"
          }
        },
        {
          "type": "FIELD",
          "name": "properties",
          "content": {
            "type": "SYMBOL",
            "name": "core_properties"
          }
        }
      ]
    },
    "core_properties": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "state",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "opening",
                "content": {
                  "type": "STRING",
                  "value": "["
                }
              },
              {
                "type": "FIELD",
                "name": "icon",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": " "
                    },
                    {
                      "type": "STRING",
                      "value": "x"
                    },
                    {
                      "type": "STRING",
                      "value": "-"
                    },
                    {
                      "type": "STRING",
                      "value": "="
                    },
                    {
                      "type": "STRING",
                      "value": "_"
                    }
                  ]
                }
              },
              {
                "type": "FIELD",
                "name": "closing",
                "content": {
                  "type": "STRING",
                  "value": "]"
                }
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "safe_text"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "FIELD",
                    "name": "description",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "icon",
                          "content": {
                            "type": "STRING",
                            "value": "$"
                          }
                        },
                        {
                          "type": "FIELD",
                          "name": "text",
                          "content": {
                            "type": "SYMBOL",
                            "name": "safe_text"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "priority",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "icon",
                          "content": {
                            "type": "STRING",
                            "value": "!"
                          }
                        },
                        {
                          "type": "FIELD",
                          "name": "number",
                          "content": {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "STRING",
                                "value": "1"
                              },
                              {
                                "type": "STRING",
                                "value": "2"
                              },
                              {
                                "type": "STRING",
                                "value": "3"
                              },
                              {
                                "type": "STRING",
                                "value": "4"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "context",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "icon",
                          "content": {
                            "type": "STRING",
                            "value": "+"
                          }
                        },
                        {
                          "type": "FIELD",
                          "name": "list",
                          "content": {
                            "type": "REPEAT1",
                            "content": {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "FIELD",
                                  "name": "text",
                                  "content": {
                                    "type": "SYMBOL",
                                    "name": "safe_text"
                                  }
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "do_date_time",
                    "content": {
                      "type": "SYMBOL",
                      "name": "do_date_or_time"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "completed_date_time",
                    "content": {
                      "type": "SYMBOL",
                      "name": "completed_date"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "id",
                    "content": {
                      "type": "SYMBOL",
                      "name": "id"
                    }
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "safe_text": {
      "type": "PATTERN",
      "value": "[^$!*+@%>#\\(]+"
    },
    "do_date_or_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "@"
          }
        },
        {
          "type": "SYMBOL",
          "name": "extended_date_and_time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "recurrance"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "completed_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "icon",
          "content": {
            "type": "STRING",
            "value": "%"
          }
        },
        {
          "type": "SYMBOL",
          "name": "date_and_time"
        }
      ]
    },
    "recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "recurrance_icon"
        },
        {
          "type": "SYMBOL",
          "name": "recurrance_structure"
        }
      ]
    },
    "recurrance_icon": {
      "type": "STRING",
      "value": "R"
    },
    "recurrance_structure": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "daily_recurrance"
        },
        {
          "type": "SYMBOL",
          "name": "weekly_recurrance"
        }
      ]
    },
    "daily_recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Da"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "weekly_recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "W"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "weekly_recurrance_days"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "time"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "weekly_recurrance_days": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "Mon"
          },
          {
            "type": "STRING",
            "value": "Tue"
          },
          {
            "type": "STRING",
            "value": "Wen"
          },
          {
            "type": "STRING",
            "value": "Thurs"
          },
          {
            "type": "STRING",
            "value": "Fri"
          },
          {
            "type": "STRING",
            "value": "Sat"
          },
          {
            "type": "STRING",
            "value": "Sun"
          }
        ]
      }
    },
    "extended_date_and_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date_and_time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "date_and_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "date": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "year"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "month"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "day"
        }
      ]
    },
    "year": {
      "type": "PATTERN",
      "value": "[0-9]{4}"
    },
    "month": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "day": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "time": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "hour"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "minute"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "am"
                },
                {
                  "type": "STRING",
                  "value": "pm"
                },
                {
                  "type": "STRING",
                  "value": "AM"
                },
                {
                  "type": "STRING",
                  "value": "PM"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "hour": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "minute": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "duration_designator"
        },
        {
          "type": "SYMBOL",
          "name": "duration_value"
        }
      ]
    },
    "duration_designator": {
      "type": "STRING",
      "value": "D"
    },
    "duration_value": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "id": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "id_icon"
        },
        {
          "type": "SYMBOL",
          "name": "uuid"
        }
      ]
    },
    "id_icon": {
      "type": "STRING",
      "value": "#"
    },
    "uuid": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "uuid_time_high"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "uuid_time_mid"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "uuid_version_random"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "uuid_variant_random"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "uuid_random"
        }
      ]
    },
    "uuid_time_high": {
      "type": "PATTERN",
      "value": "[0-9a-f]{8}"
    },
    "uuid_time_mid": {
      "type": "SYMBOL",
      "name": "_uuid_chunk"
    },
    "uuid_version_random": {
      "type": "SYMBOL",
      "name": "_uuid_chunk"
    },
    "uuid_variant_random": {
      "type": "SYMBOL",
      "name": "_uuid_chunk"
    },
    "uuid_random": {
      "type": "PATTERN",
      "value": "[0-9a-f]{12}"
    },
    "_uuid_chunk": {
      "type": "PATTERN",
      "value": "[0-9a-f]{4}"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}