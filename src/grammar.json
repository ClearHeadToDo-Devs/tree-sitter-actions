{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "actions",
  "rules": {
    "action_list": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "root_action"
      }
    },
    "root_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "root_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "story",
              "content": {
                "type": "SYMBOL",
                "name": "story"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "children",
              "content": {
                "type": "SYMBOL",
                "name": "child_action_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "child_action_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "child_action"
      }
    },
    "child_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "FIELD",
          "name": "child_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "grandchildren",
              "content": {
                "type": "SYMBOL",
                "name": "grandchild_action_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "grandchild_action_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "grandchild_action"
      }
    },
    "grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>"
        },
        {
          "type": "FIELD",
          "name": "grandchild_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "great_grandchildren",
              "content": {
                "type": "SYMBOL",
                "name": "great_grandchild_action_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "great_grandchild_action_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "great_grandchild_action"
      }
    },
    "great_grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>"
        },
        {
          "type": "FIELD",
          "name": "great_grandchild_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "double_great_grandchildren",
              "content": {
                "type": "SYMBOL",
                "name": "double_great_grandchild_action_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "double_great_grandchild_action_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "double_great_grandchild_action"
      }
    },
    "double_great_grandchild_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>>"
        },
        {
          "type": "FIELD",
          "name": "double_great_grandchild_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "leaves",
              "content": {
                "type": "SYMBOL",
                "name": "leaf_action_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "leaf_action_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "leaf_action"
      }
    },
    "leaf_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>>>>"
        },
        {
          "type": "FIELD",
          "name": "leaf_props",
          "content": {
            "type": "SYMBOL",
            "name": "core_action"
          }
        }
      ]
    },
    "core_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "state",
          "content": {
            "type": "SYMBOL",
            "name": "state"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "description",
              "content": {
                "type": "SYMBOL",
                "name": "description"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "priority",
              "content": {
                "type": "SYMBOL",
                "name": "priority"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "context_list",
              "content": {
                "type": "SYMBOL",
                "name": "context_list"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "do_date_time",
              "content": {
                "type": "SYMBOL",
                "name": "do_date_or_time"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "completed_date_time",
              "content": {
                "type": "SYMBOL",
                "name": "completed_date"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "id",
              "content": {
                "type": "SYMBOL",
                "name": "id"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "state": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "not_started"
            },
            {
              "type": "SYMBOL",
              "name": "completed"
            },
            {
              "type": "SYMBOL",
              "name": "in_progress"
            },
            {
              "type": "SYMBOL",
              "name": "blocked"
            },
            {
              "type": "SYMBOL",
              "name": "cancelled"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "not_started": {
      "type": "STRING",
      "value": " "
    },
    "completed": {
      "type": "STRING",
      "value": "x"
    },
    "in_progress": {
      "type": "STRING",
      "value": "-"
    },
    "blocked": {
      "type": "STRING",
      "value": "="
    },
    "cancelled": {
      "type": "STRING",
      "value": "_"
    },
    "name": {
      "type": "PATTERN",
      "value": "[^$!*+@%>#\\(]+"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "SYMBOL",
          "name": "description_text"
        }
      ]
    },
    "description_text": {
      "type": "PATTERN",
      "value": "[^!*+@%>#\\(]+"
    },
    "priority": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "SYMBOL",
          "name": "priority_number"
        }
      ]
    },
    "priority_number": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "story": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "story_name"
        }
      ]
    },
    "story_name": {
      "type": "PATTERN",
      "value": "[^+@%>#\\(]+"
    },
    "context_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "middle_context"
              },
              {
                "type": "SYMBOL",
                "name": "context_text"
              }
            ]
          }
        }
      ]
    },
    "middle_context": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "context_text"
        },
        {
          "type": "STRING",
          "value": ","
        }
      ]
    },
    "context_text": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9\\-_]+"
    },
    "do_date_or_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "extended_date_and_time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "recurrance"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "completed_date": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "SYMBOL",
          "name": "date_and_time"
        }
      ]
    },
    "recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "R"
        },
        {
          "type": "SYMBOL",
          "name": "recurrance_structure"
        }
      ]
    },
    "recurrance_structure": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "daily_recurrance"
        },
        {
          "type": "SYMBOL",
          "name": "weekly_recurrance"
        }
      ]
    },
    "daily_recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Da"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "weekly_recurrance": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "W"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "weekly_recurrance_days"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "time"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "weekly_recurrance_days": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "Mon"
          },
          {
            "type": "STRING",
            "value": "Tue"
          },
          {
            "type": "STRING",
            "value": "Wen"
          },
          {
            "type": "STRING",
            "value": "Thurs"
          },
          {
            "type": "STRING",
            "value": "Fri"
          },
          {
            "type": "STRING",
            "value": "Sat"
          },
          {
            "type": "STRING",
            "value": "Sun"
          }
        ]
      }
    },
    "extended_date_and_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date_and_time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "date_and_time": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "date": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "year"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "month"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "day"
        }
      ]
    },
    "year": {
      "type": "PATTERN",
      "value": "[0-9]{4}"
    },
    "month": {
      "type": "PATTERN",
      "value": "[0-9]{2}"
    },
    "day": {
