{
  "name": "actions",
  "rules": {
    "action_list": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "action"
      }
    },
    "action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "state"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "priority"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "story"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "context_list"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "state": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "not_started"
            },
            {
              "type": "SYMBOL",
              "name": "completed"
            },
            {
              "type": "SYMBOL",
              "name": "in_progress"
            },
            {
              "type": "SYMBOL",
              "name": "blocked"
            },
            {
              "type": "SYMBOL",
              "name": "cancelled"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "not_started": {
      "type": "STRING",
      "value": " "
    },
    "completed": {
      "type": "STRING",
      "value": "x"
    },
    "in_progress": {
      "type": "STRING",
      "value": "-"
    },
    "blocked": {
      "type": "STRING",
      "value": "="
    },
    "cancelled": {
      "type": "STRING",
      "value": "_"
    },
    "name": {
      "type": "PATTERN",
      "value": "[^$!*+@%>]+"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "PATTERN",
          "value": "[^!*+@%>]+"
        }
      ]
    },
    "priority": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "PATTERN",
          "value": "[0-9]+"
        }
      ]
    },
    "story": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "PATTERN",
          "value": "[^+@%>]+"
        }
      ]
    },
    "context_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "middle_context"
              },
              {
                "type": "SYMBOL",
                "name": "tail_context"
              }
            ]
          }
        }
      ]
    },
    "middle_context": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9\\-_]+"
        },
        {
          "type": "STRING",
          "value": ","
        }
      ]
    },
    "tail_context": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9\\-_]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
